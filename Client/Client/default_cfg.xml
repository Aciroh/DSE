    <memory architecture="l2">
      <l1_code hitrate="0.990" latency="1" />
      <l1_data hitrate="0.970" latency="1" />
      <l2 hitrate="0.990" latency="3" />
      <system latency="20" />
    </memory>
  </config>
</psatsim>